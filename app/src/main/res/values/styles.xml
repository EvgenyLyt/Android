<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="CheckItem" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/colorRed</item>
        <item name="colorControlActivated">@color/colorGreen</item>
    </style>
    <style name="CheckName" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/colorGray</item>
        <item name="colorControlActivated">@color/colorDarkGreen</item>
    </style>

</resources>
